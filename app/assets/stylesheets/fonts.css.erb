@font-face {
    font-family: 'OratorStd';
    src: url('<%= asset_path('OratorStd.eot') %>');
    src: url('<%= asset_path('OratorStd.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('OratorStd.woff') %>') format('woff'),
         url('<%= asset_path('OratorStd.ttf') %>') format('truetype'),
         url('<%= asset_path('OratorStd.svg') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
